{
  "name": "tms-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target=app",
    "build:stage": "vue-cli-service build --mode staging --target=app",
    "build:preview":"npm run build:stage && serve -s dist",
    "test": "vue-cli-service test",
    "lint": "vue-cli-service lint",
    "build:stage:docker": "npm run build:stage && docker build -t really_edu/tms-web .",
    "build:docker": "npm run build && docker build -t really_edu/tms-web ."
  },
  "dependencies": {
    "axios": "^0.18.0",
    "echarts": "^4.1.0",
    "element-ui": "^2.4",
    "moment": "^2.22.1",
    "qs": "^6.5.1",
    "v-charts": "^1.16.20",
    "vue": "^2.5.13",
    "vue-headful": "^2.0.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-beta.6",
    "@vue/cli-service": "latest",
    "@vue/test-utils": "^1.0.0-beta.10",
    "chai": "^4.1.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
